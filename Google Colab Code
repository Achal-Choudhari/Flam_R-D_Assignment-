    {
      "cell_type": "code",
      "source": [
        "# Stage 2: estimating t_i and M for the assignment equation\n",
        "t_est = (x - X) / np.cos(theta)\n",
        "t_est = np.clip(t_est, 6.0, 60.0)\n",
        "\n",
        "res_y = y - (42.0 + t_est * np.sin(theta))\n",
        "a_est = res_y / np.cos(theta)\n",
        "\n",
        "sin03t = np.sin(0.3 * t_est)\n",
        "mask = np.abs(sin03t) > 0.12\n",
        "vals = a_est[mask] / sin03t[mask]\n",
        "\n",
        "valid_idx = (vals > 0) & np.isfinite(vals)\n",
        "tt = np.abs(t_est[mask][valid_idx])\n",
        "vals_pos = vals[valid_idx]\n",
        "\n",
        "if len(vals_pos) < 10:\n",
        "    print(\"Not enough reliable points to estimate M. Trying to lowering mask threshold.\")\n",
        "    M = 0.0\n",
        "else:\n",
        "    p = np.polyfit(tt, np.log(vals_pos), 1)\n",
        "    M, c = p\n",
        "\n",
        "print(\"Estimated M = \", M)\n"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
