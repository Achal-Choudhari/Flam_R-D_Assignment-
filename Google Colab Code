    {
      "cell_type": "code",
      "source": [
        "import matplotlib.pyplot as plt\n",
        "\n",
        "e_term = np.exp(np.abs(M * t_est)) * np.sin(0.3 * t_est)\n",
        "x_pred = t_est * np.cos(theta) - e_term * np.sin(theta) + X\n",
        "y_pred = 42.0 + t_est * np.sin(theta) + e_term * np.cos(theta)\n",
        "\n",
        "L1 = np.mean(np.abs(x_pred - x) + np.abs(y_pred - y))\n",
        "print(\" The Average L1 distance (mean |dx|+|dy|) = \", L1)\n",
        "\n",
        "plt.figure(figsize=(10,5))\n",
        "plt.subplot(1,2,1)\n",
        "plt.scatter(x, y, s=6, label='data')\n",
        "plt.scatter(x_pred, y_pred, s=5, alpha=0.6, label='model', marker='x')\n",
        "plt.title('Data vs Model')\n",
        "plt.legend()\n",
        "plt.axis('equal')\n",
        "\n",
        "plt.subplot(1,2,2)\n",
        "plt.plot(t_est, a_est, '.', ms=3, label='a_est = res_y/cos(Î¸)')\n",
        "plt.plot(tt, vals_pos, 'ro', ms=3, label='used for M fit')\n",
        "plt.xlabel('t')\n",
        "plt.legend()\n",
        "plt.tight_layout()\n",
        "plt.show()\n"
