      "cell_type": "code",
      "source": [
        "import math\n",
        "from math import atan, sin, cos, tan\n",
        "\n",
        "# Stage 1: linear fit y = b + sÂ·x\n",
        "A = np.vstack([np.ones(N), x]).T\n",
        "coeffs, *_ = np.linalg.lstsq(A, y, rcond=None)\n",
        "b, s = coeffs\n",
        "\n",
        "theta = np.arctan(s)\n",
        "theta_deg = np.degrees(theta)\n",
        "\n",
        "if abs(np.tan(theta)) < 1e-8:\n",
        "    raise ValueError(\"Estimated theta leads to tan(theta) â‰ˆ 0; can't compute X robustly.\")\n",
        "\n",
        "X = (42.0 - b) / np.tan(theta)\n",
        "\n",
        "print(\"ðŸ“ˆ Linear regression results:\")\n",
        "print(f\"  slope s = {s}\")\n",
        "print(f\"  intercept b = {b}\")\n",
        "print(f\"  Î¸ = {theta:.6f} rad ({theta_deg:.6f} Â°)\")\n",
        "print(f\"  X = {X:.6f}\")\n",
        "\n",
        "if not (0 < theta_deg < 50):\n",
        "    print(\"Warning: Î¸ out of allowed range (0â€“50Â°)\")\n"
      ],
